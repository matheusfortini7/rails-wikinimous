<h1> <%= @article.title %></h1>
<p> <%= @article.content %> </p>
<%= link_to 'Back', articles_path, class: "btn btn-primary" %>
<%= link_to 'Edit', edit_article_path, class: "btn btn-dark" %>
<%= link_to 'Delete', article_path(@article), method: 'delete', data: { confirm: 'Remove article?' }, class: "btn btn-danger" %>
